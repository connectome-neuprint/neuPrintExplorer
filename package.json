  "scripts": {
    "pretest": "eslint --ext .jsx,.js src/js/",
    "test": "jest --maxWorkers=4",
    "test-watch": "jest --watch",
    "preversion": "npm test",
    "dev-server": "webpack-dev-server --open",
    "dev": "docker-compose up -d && webpack",
    "build": "webpack --config webpack.prod.js",
    "size": "size-limit",
    "build-analysis": "webpack --config webpack.analysis.js",
    "build-public": "webpack --config webpack.public.js",
    "dev-public": "webpack --config webpack.public-dev.js",
    "prepare": "husky install"
  },
  "size-limit": [
    {
      "limit": "3 MB",
      "path": "build/js/app.*.js"
    }
  ],
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "singleQuote": true
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ],
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^helpers(.*)$": "<rootDir>/src/js/helpers$1",
      "^plugins(.*)$": "<rootDir>/src/js/components/plugins$1",
      "^views(.*)$": "<rootDir>/src/js/components/view-plugins$1",
      "^actions(.*)$": "<rootDir>/src/js/actions$1"
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}
